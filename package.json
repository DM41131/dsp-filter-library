{
  "name": "dsp-filter-library",
  "version": "1.1.0",
  "type": "module",
  "description": "A comprehensive DSP library with 23 window functions, advanced FIR/IIR filter design, biquad combination, and interactive visualization",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "browser": "lib/dsp-filter-library.min.js",
  "unpkg": "lib/dsp-filter-library.min.js",
  "jsdelivr": "lib/dsp-filter-library.min.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts",
      "browser": "./lib/dsp-filter-library.min.js",
      "unpkg": "./lib/dsp-filter-library.min.js",
      "jsdelivr": "./lib/dsp-filter-library.min.js"
    },
    "./dsp-filter-library": {
      "import": "./lib/dsp-filter-library.js",
      "require": "./lib/dsp-filter-library.cjs",
      "types": "./lib/dsp-filter-library.d.ts"
    },
    "./browser": {
      "import": "./lib/dsp-filter-library.esm.min.js",
      "require": "./lib/dsp-filter-library.min.js"
    },
    "./min": {
      "import": "./lib/dsp-filter-library.esm.min.js",
      "require": "./lib/dsp-filter-library.min.js"
    },
    "./core/Complex": {
      "import": "./lib/core/Complex.js",
      "require": "./lib/core/Complex.cjs",
      "types": "./lib/core/Complex.d.ts"
    },
    "./core/util": {
      "import": "./lib/core/util.js",
      "require": "./lib/core/util.cjs",
      "types": "./lib/core/util.d.ts"
    },
    "./core/Polynomial": {
      "import": "./lib/core/Polynomial.js",
      "require": "./lib/core/Polynomial.cjs",
      "types": "./lib/core/Polynomial.d.ts"
    },
    "./core/Roots": {
      "import": "./lib/core/Roots.js",
      "require": "./lib/core/Roots.cjs",
      "types": "./lib/core/Roots.d.ts"
    },
    "./analog/Prototypes": {
      "import": "./lib/analog/Prototypes.js",
      "require": "./lib/analog/Prototypes.cjs",
      "types": "./lib/analog/Prototypes.d.ts"
    },
    "./digital/BLT": {
      "import": "./lib/digital/BLT.js",
      "require": "./lib/digital/BLT.cjs",
      "types": "./lib/digital/BLT.d.ts"
    },
    "./digital/SOS": {
      "import": "./lib/digital/SOS.js",
      "require": "./lib/digital/SOS.cjs",
      "types": "./lib/digital/SOS.d.ts"
    },
    "./digital/Response": {
      "import": "./lib/digital/Response.js",
      "require": "./lib/digital/Response.cjs",
      "types": "./lib/digital/Response.d.ts"
    },
    "./fir/Windows": {
      "import": "./lib/fir/Windows.js",
      "require": "./lib/fir/Windows.cjs",
      "types": "./lib/fir/Windows.d.ts"
    },
    "./fir/FIRDesigner": {
      "import": "./lib/fir/FIRDesigner.js",
      "require": "./lib/fir/FIRDesigner.cjs",
      "types": "./lib/fir/FIRDesigner.d.ts"
    },
    "./fir/FIRZeros": {
      "import": "./lib/fir/FIRZeros.js",
      "require": "./lib/fir/FIRZeros.cjs",
      "types": "./lib/fir/FIRZeros.d.ts"
    },
    "./iir/IIRDesigner": {
      "import": "./lib/iir/IIRDesigner.js",
      "require": "./lib/iir/IIRDesigner.cjs",
      "types": "./lib/iir/IIRDesigner.d.ts"
    },
    "./model/FIRFilter": {
      "import": "./lib/model/FIRFilter.js",
      "require": "./lib/model/FIRFilter.cjs",
      "types": "./lib/model/FIRFilter.d.ts"
    },
    "./model/IIRFilter": {
      "import": "./lib/model/IIRFilter.js",
      "require": "./lib/model/IIRFilter.cjs",
      "types": "./lib/model/IIRFilter.d.ts"
    }
  },
  "files": [
    "lib",
    "src",
    "docs",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "rollup -c scripts/rollup.config.js --format es",
    "build:cjs": "rollup -c scripts/rollup.config.js --format cjs",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib",
    "build:min": "npm run clean && rollup -c scripts/rollup.config.js",
    "build:all": "npm run build && npm run build:min",
    "prepublishOnly": "npm run build:all",
    "test": "node examples/test/example.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "clean": "rimraf lib",
    "prepack": "npm run build:all",
    "version": "npm run build:all && git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "dsp",
    "digital signal processing",
    "filter",
    "fir",
    "iir",
    "fft",
    "butterworth",
    "chebyshev",
    "elliptic",
    "bessel",
    "linkwitz-riley",
    "window functions",
    "kaiser",
    "tukey",
    "gaussian",
    "blackman-harris",
    "biquad",
    "second-order sections",
    "complex numbers",
    "signal processing",
    "audio processing",
    "interactive demo"
  ],
  "author": {
    "name": "Davit Akobia",
    "email": "dav.akobia@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dsp-filter-library.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/dsp-filter-library/issues"
  },
  "homepage": "https://github.com/yourusername/dsp-filter-library#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
