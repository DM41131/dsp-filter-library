{
  "name": "dsp-filter-library",
  "version": "1.0.0",
  "type": "module",
  "description": "A modular digital signal processing library with FIR/IIR filter design, FFT operations, and complex number utilities",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "browser": "lib/dsp-filter-library.min.js",
  "unpkg": "lib/dsp-filter-library.min.js",
  "jsdelivr": "lib/dsp-filter-library.min.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts",
      "browser": "./lib/dsp-filter-library.min.js",
      "unpkg": "./lib/dsp-filter-library.min.js",
      "jsdelivr": "./lib/dsp-filter-library.min.js"
    },
    "./browser": {
      "import": "./lib/dsp-filter-library.esm.min.js",
      "require": "./lib/dsp-filter-library.min.js"
    },
    "./min": {
      "import": "./lib/dsp-filter-library.esm.min.js",
      "require": "./lib/dsp-filter-library.min.js"
    },
    "./complex": {
      "import": "./lib/complex.js",
      "require": "./lib/complex.cjs",
      "types": "./lib/complex.d.ts"
    },
    "./fft": {
      "import": "./lib/fft.js",
      "require": "./lib/fft.cjs",
      "types": "./lib/fft.d.ts"
    },
    "./fir": {
      "import": "./lib/fir.js",
      "require": "./lib/fir.cjs",
      "types": "./lib/fir.d.ts"
    },
    "./iir": {
      "import": "./lib/iir.js",
      "require": "./lib/iir.cjs",
      "types": "./lib/iir.d.ts"
    },
    "./filter": {
      "import": "./lib/filter.js",
      "require": "./lib/filter.cjs",
      "types": "./lib/filter.d.ts"
    },
    "./windows": {
      "import": "./lib/windows.js",
      "require": "./lib/windows.cjs",
      "types": "./lib/windows.d.ts"
    },
    "./utils": {
      "import": "./lib/utils.js",
      "require": "./lib/utils.cjs",
      "types": "./lib/utils.d.ts"
    },
    "./zdomain": {
      "import": "./lib/zdomain.js",
      "require": "./lib/zdomain.cjs",
      "types": "./lib/zdomain.d.ts"
    }
  },
  "files": [
    "lib",
    "src",
    "docs",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "rollup -c scripts/rollup.config.js --format es",
    "build:cjs": "rollup -c scripts/rollup.config.js --format cjs",
    "build:types": "copy src\\types.d.ts lib\\index.d.ts",
    "build:min": "npm run clean && rollup -c scripts/rollup.config.js",
    "build:all": "npm run build && npm run build:min",
    "prepublishOnly": "npm run build:all",
    "test": "node examples/test/example.js",
    "lint": "eslint src/*.js",
    "lint:fix": "eslint src/*.js --fix",
    "clean": "rimraf lib",
    "prepack": "npm run build:all",
    "version": "npm run build:all && git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "dsp",
    "digital signal processing",
    "filter",
    "fir",
    "iir",
    "fft",
    "butterworth",
    "chebyshev",
    "window functions",
    "complex numbers",
    "signal processing",
    "audio processing"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dsp-filter-library.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/dsp-filter-library/issues"
  },
  "homepage": "https://github.com/yourusername/dsp-filter-library#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
